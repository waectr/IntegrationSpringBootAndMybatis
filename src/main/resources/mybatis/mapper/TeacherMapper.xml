        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.waectr.springbootmybatis.mapper.TeacherMapper">

    <select id="getTeacherById" resultType="com.waectr.springbootmybatis.bean.Teacher">
